package co.com.techandsolve.creditcard.fizzbuzz;


public class Fizz {
	

	public String get(int i) {
		String result = null;
		if(i ==1 || i ==2 || i==4 || i==5) result = "Fizz";
		if(i ==3 || i == 6) result = "Buzz";

		return result;
	}
	
	public String getWithFor(int value){
	   if(value%3==0) return "Buzz";
	   return "Fizz";
	}

}
